  "scripts": {
    "app": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "serverapp": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electronserver\"",
    "apphelp": "electron . --cmd help",
    "electronforge": "electron-forge start",
    "electronb": "electron .",
    "electrona": "wait-on http://127.0.0.1:3000 && electron .",
    "electron": "wait-on http://127.0.0.1:3000 && electron-forge start",
    "electronserver": "wait-on http://127.0.0.1:3000 && electron-forge start -- -o server",
    "devenv": "gulp devenv --max_old_space_size=8192",
    "web": "react-scripts --max_old_space_size=8192 start",
    "start": "react-scripts --max_old_space_size=8192 start",
    "webbuild": "react-scripts --max_old_space_size=8192 build",
    "vscbuild": "gulp vscbuild",
    "vsccorebuild": "gulp vsccorebuild",
    "jsnbuild": "gulp jsnbuild",
    "jsncorebuild": "gulp jsncorebuild",
    "appbuild": "react-scripts --max_old_space_size=8192 build && electron-forge package",
    "copyctforgeconfig": "node -e \"const fs = require('fs'); const data = fs.readFileSync('creatortools.forge.config.js'); fs.writeFileSync('forge.config.js', data);\"",
    "copyserverforgeconfig": "node -e \"const fs = require('fs'); const data = fs.readFileSync('server.forge.config.js'); fs.writeFileSync('forge.config.js', data);\"",
    "appbuildandsetup": "npm run-script copyctforgeconfig && react-scripts --max_old_space_size=8192 build && electron-forge make",
    "serverbuildandsetup": "npm run-script copyserverforgeconfig && react-scripts --max_old_space_size=8192 build && electron-forge make",
    "test": "ts-mocha -delay -p tsconfig.test.json src/test/**/*.ts",
    "test-ci": "ts-mocha -delay -p tsconfig.test.json src/test/**/*.ts --reporter json > test/test-results.json",
    "eject": "react-scripts eject",
    "vsc-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=./toolbuild/vsc/ ../tests/empty/",
    "package-vsccore": "webpack --config webpack.vsccore.config.js --mode production --devtool hidden-source-map"
  },
  "bin": {
    "mct": "./toolbuild/jsn/cli/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
